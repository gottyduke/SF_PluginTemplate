[
	{
        "action": "copy",
        "params": [
            "{dist}\\*.asi",
            "{dist}\\Plugins\\"
        ]
    },
	{
        "action": "copy_if",
        "params": [
            "{dist}\\*.ini",
            "{dist}\\Plugins\\"
        ]
    },
	{
        "action": "copy_if",
        "params": [
            "{dist}\\*.json",
            "{dist}\\Plugins\\"
        ]
    },
	{
        "action": "copy_if",
        "params": [
            "{dist}\\*.toml",
            "{dist}\\Plugins\\"
        ]
    },
    {
        "action": "package",
        "config": "Release",
        "params": [
            "{dist}\\Plugins\\",
            "{dist}\\{project_name}.v{project_version}.zip"
        ]
    },
    {
        "action": "package",
        "config": "RelWithDebInfo",
        "params": [
            "{dist}\\Plugins\\",
            "{dist}\\{project_name}.v{project_version}.zip"
        ]
    }
]